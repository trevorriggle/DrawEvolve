<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Distribution method: app-store for TestFlight/App Store -->
    <key>method</key>
    <string>app-store</string>

    <!-- Replace with your Team ID from Apple Developer Portal -->
    <!-- Example: ABC123DEF4 -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>

    <!-- Signing style: automatic (managed by Xcode) -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Don't upload bitcode (deprecated by Apple) -->
    <key>uploadBitcode</key>
    <false/>

    <!-- Upload debug symbols for crash reports -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Manage version and build numbers automatically -->
    <key>manageAppVersionAndBuildNumber</key>
    <false/>

    <!-- Compile bitcode (if needed) -->
    <key>compileBitcode</key>
    <false/>
</dict>
</plist>
